                                        <label for="classMixed">
                                            Mista
                                        </label>
                                    </div>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <button type="button" class="btn btn-lg btn-default btn-search">Nova Busca</button>
        </div>
    </form>
