    ViewBag.BodyClass = "flux-collapsed";
}


<div class="container-fluid packages search-result top">
    <!-- Busca -->
    <form class="search clearfix package-search">
        <div class="container-fluid">
            <div class="checkin-checkout pull-left">
                <div class="flight clearfix">
                    <div class="wrapper-init">
                        <div class="col destinys from">
                            <input type="text" class="suggestions">
                        </div>
                        <div class="col destinys to">
                            <input type="text" class="suggestions">
                        </div>
                    </div>
                    <div class="col dates jrange">
                        <div class="wrap-dates">
                            <label>Voo</label>
                            <div class="data date-init"></div>
                            <i></i>
                            <div class="data date-end"></div>
                        </div>
                        <div class="calendar"></div>
                    </div>
                </div>

                <div class="accommodation">
                    <div class="col hotel">
                        <input type="text" class="suggestions">
                    </div>
                    <div class="col dates jrange">
                        <div class="wrap-dates">
                            <label>Hotel</label>
                            <div class="data date-init">Entrada</div>
                            <i></i>
                            <div class="data date-end">Saída</div>
                        </div>
                        <div class="calendar"></div>
                    </div>
                </div>
            </div>
            <div class="options hotel-option">
                <button type="button" class="btn btn-md btn-text-cta btn-expand options options-button click">
                    <i class="glyphicon glyphicon-icon-doorknob"></i>
                    <span class="options-button"><span class="number">3</span> Quartos</span>
                </button>
                <div class="options-pop clearfix">
                    <div class="top-fixed">
                        <div class="add-room">
                            <div class="border">
                                <button type="button" class="btn btn-sm btn-text-icon"><span class="glyphicon glyphicon-plus-white"></span>Adicionar quarto</button>
                            </div>
                        </div>
                        <table class="row-room">
                            <tr>
                                <td class="th-room" data-room="room-1">
                                    <button type="button" class="btn">
                                        <span class="text">Quarto 1</span>
                                        <span class="glyphicon glyphicon-btn-close"></span>
                                    </button>
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div class="scroll">
                        <table class="room-people content-1">
                            <tr>
                                <td data-room="room-1">
                                    <label class="spinner-wrapper">
                                        Adulto
                                        <input class="spinner spinner-simple" name="value" value="0">
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td data-room="room-1">
                                    <label class="spinner-wrapper">
                                        Criança
                                        <input class="spinner spinner-simple spinner-child" name="childAge" value="0">
                                    </label>
                                </td>
                            </tr>
                            <tr class="age hidden">
                                <td data-room="room-1"></td>
                            </tr>
                        </table>
                        <div class="options-flight">
                            <div class="radio trip dark-theme">
                                <div>
                                    <input type="radio" checked="" value="roundTrip" name="typeTrip" id="roundTrip">
                                    <label for="roundTrip">Ida e volta</label>
                                </div>
                                <div>
                                    <input type="radio" value="onewWayTrip" name="typeTrip" id="onewWayTrip">
